{
 :paths   ["src"]

 :deps    {
           org.clojure/clojure     {:mvn/version "1.10.0"}
           clojure-csv/clojure-csv {:mvn/version "2.0.2"}
           orchestra               {:mvn/version "2019.02.06-1"}
           }

 :aliases {
           :test       {:extra-paths ["test"]}
           :test-check {:extra-deps {org.clojure/test.check {:mvn/version "0.9.0"}}}
           :cider-clj  {:extra-deps {cider/cider-nrepl {:mvn/version "0.21.0"}}
                        :main-opts  ["-m" "nrepl.cmdline" "--middleware" "[cider.nrepl/cider-middleware]"]}
           }
 }

